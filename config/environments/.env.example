# WhatsApp Google Uploader - Example Configuration
# AIDEV-NOTE: env-example; example configuration with safe values

NODE_ENV=development
LOG_LEVEL=info

# Google API Configuration (replace with your values)
GOOGLE_CLIENT_ID=123456789-abcdef.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-example_client_secret
GOOGLE_REDIRECT_URI=urn:ietf:wg:oauth:2.0:oob

# Default paths
DATA_DIR=~/.whatsapp-uploader
WHATSAPP_DIR=/storage/emulated/0/WhatsApp/Media
DATABASE_PATH=${DATA_DIR}/uploader.db
TOKEN_PATH=${DATA_DIR}/tokens.json
LOG_PATH=${DATA_DIR}/logs

# Upload settings
DEFAULT_BATCH_SIZE=10
DEFAULT_CONCURRENT_UPLOADS=3
MAX_RETRY_ATTEMPTS=3
RATE_LIMIT_DELAY=1000

# Security
ENCRYPT_TOKENS=true
TOKEN_ENCRYPTION_KEY=example32characterencryptionkeyher

# Performance
STREAM_CHUNK_SIZE=65536
MAX_MEMORY_USAGE=256
ENABLE_COMPRESSION=true

# Debug settings
DEBUG_MODE=false
VERBOSE_LOGGING=false
MOCK_GOOGLE_APIS=false
DRY_RUN_DEFAULT=false